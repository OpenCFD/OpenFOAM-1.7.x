FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      N2;
}
// ************************************************************************* //

dimensions          [ 0 0 0 0 0 0 0 ];

internalField       uniform 0.766;

referenceLevel      0;

boundaryField
{
    walls
    {
        type                zeroGradient;
    }

    front
    {
        type                wedge;
    }

    back
    {
        type                wedge;
    }

}

