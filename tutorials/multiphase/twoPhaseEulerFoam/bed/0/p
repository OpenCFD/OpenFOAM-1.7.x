FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [ 1 -1 -2 0 0 0 0 ];

internalField       uniform 0;

boundaryField
{
    bottom
    {
        type            buoyantPressure;
        value           uniform 0;
    }

    top
    {
        type               fixedValue;
        value              uniform 0;
    }

    walls
    {
        type            buoyantPressure;
        value           uniform 0;
    }

    frontBack
    {
        type               empty;
    }
}

