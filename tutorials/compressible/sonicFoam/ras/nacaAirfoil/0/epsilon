FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 25000;

boundaryField
{
    INLE1           
    {
        type            inletOutlet;
        inletValue      uniform 25000;
        value           uniform 25000;
    }

    OUTL2           
    {
        type            inletOutlet;
        inletValue      uniform 25000;
        value           uniform 25000;
    }

    SYMP3           
    {
        type            empty;
    }

    WALL10          
    {
        type            zeroGradient;
    }
}

