FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      mut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 -1 -1 0 0 0 0 ];

internalField   uniform 0;

boundaryField
{
    front
    {
        type            mutWallFunction;
        value           uniform 0;
    }

    back
    {
        type            mutWallFunction;
        value           uniform 0;
    }

    wall
    {
        type            mutWallFunction;
        value           uniform 0;
    }

    porosityWall
    {
        type            mutWallFunction;
        value           uniform 0;
    }

    inlet
    {
        type            calculated;
        value           uniform 0;
    }

    outlet
    {
        type            calculated;
        value           uniform 0;
    }
}


