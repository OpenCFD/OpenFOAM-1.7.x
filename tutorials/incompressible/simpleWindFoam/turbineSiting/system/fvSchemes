FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default             steadyState;
}

gradSchemes
{
    default             Gauss linear;
    grad(p)             Gauss linear;
    grad(U)             Gauss linear;
}

divSchemes
{
    default             none;
    div(phi,U)          Gauss upwind grad(U);
    div((nuEff*dev(grad(U).T())))   Gauss linear;
    div(phi,epsilon)    Gauss upwind;
    div(phi,k)          Gauss upwind;
}

laplacianSchemes
{
    default             Gauss linear limited 0.333;
}

interpolationSchemes
{
    default             linear;
}

snGradSchemes
{
    default             limited 0.333;
}

fluxRequired
{
    default             no;
    p;
}


// ************************************************************************* //
