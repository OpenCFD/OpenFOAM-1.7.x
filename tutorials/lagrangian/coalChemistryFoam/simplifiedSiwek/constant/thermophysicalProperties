foamChemistryFile "$FOAM_CASE/chemkin/foam.inp";

inertSpecie     N2;

liquidComponents
(
    H2O
);

H2O             H2O defaultCoeffs;

solidComponents
(
    C
    ash
);

C               C defaultCoeffs;

ash             ash defaultCoeffs;


